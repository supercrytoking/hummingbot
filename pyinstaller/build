#!/usr/bin/env python

from os.path import (
    realpath,
    join
)
import shutil


def project_dir() -> str:
    return realpath(join(__file__, "../../"))


def main():
    shutil.move(join(project_dir(), "bin/hummingbot.py"), join(project_dir(), "bin/bot"))
    try:
        pass
    finally:
        shutil.move(join(project_dir(), "bin/bot"), join(project_dir(), "bin/hummingbot.py"))


if __name__ == "__main__":
    main()
